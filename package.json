    "eslint": "^10.0.0",
    "eslint-plugin-astro": "^1.5.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.5.2",
    "prettier": "^3.8.1",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "typescript-eslint": "^8.56.0"
