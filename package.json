    "@tailwindcss/vite": "^4.1.18",
    "astro": "^5.17.2",
    "astro-mermaid": "^1.3.1",
    "fuse.js": "^7.1.0",
    "github-slugger": "^2.0.0",
    "markdown-it": "^14.1.1",
    "mermaid": "^11.12.2",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "sanitize-html": "^2.17.0",
    "satori": "^0.19.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@astrojs/react": "^4.4.2",
